s\ninterface GpioState {\n    [key: number]: number;\n}\nlet gpioState: GpioState = {};\nfunction digitalWrite(pin: number, value: number): void {\n    gpioState[pin] = value;\n}\nfunction digitalRead(pin: number): number {\n    return gpioState[pin] || 0;\n}\n\n// Fonction pour avancer\nfunction avancer(): void {\n    digitalWrite(IN1, 1);\n    digitalWrite(IN2, 0);\n    digitalWrite(IN3, 1);\n    digitalWrite(IN4, 0);\n    digitalWrite(ENA, 1);\n    digitalWrite(ENB, 1);\n}\n\n// Fonction pour tourner à gauche\nfunction tournerGauche(): void {\n    digitalWrite(IN1, 0);\n    digitalWrite(IN2, 1);\n    digitalWrite(IN3, 1);\n    digitalWrite(IN4, 0);\n}\n\n// Fonction pour tourner à droite\nfunction tournerDroite(): void {\n    digitalWrite(IN1, 1);\n    digitalWrite(IN2, 0);\n    digitalWrite(IN3, 0);\n    digitalWrite(IN4, 1);\n}\n\n// Fonction pour arrêter\nfunction arreter(): void {\n    digitalWrite(IN1, 0);\n    digitalWrite(IN2, 0);\n    digitalWrite(IN3, 0);\n    digitalWrite(IN4, 0);\n}\n\n// Boucle principale\nsetInterval((): void => {\n    let gauche: number = digitalRead(IR_LEFT);\n    let droite: number = digitalRead(IR_RIGHT);\n\n    if (gauche === 0 && droite === 0) {\n        avancer();\n    } else if (gauche === 1 && droite === 0) {\n        tournerDroite();\n    } else if (gauche === 0 && droite === 1) {\n        tournerGauche();\n    } else {\n        arreter();\n    }\n}, 100);\n","README.md":"","pxt.json":"{\n    \"name\": \"Sans titre\",\n    \"description\": \"\",\n    \"dependencies\": {\n        \"core\": \"*\",\n        \"radio\": \"*\",\n        \"microphone\": \"*\"\n    },\n    \"files\": [\n        \"main.blocks\",\n        \"main.ts\",\n        \"README.md\"\n    ],\n    \"preferredEditor\": \"tsprj\"\n}\n"}}],"shares":[],"lastSaveTime":1738573034841}